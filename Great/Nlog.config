<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
  <targets>
    <target name="ConsoleLog" xsi:type="ColoredConsole" Layout="${longdate}|${level:uppercase=true}|${message}"/>
    <target name="GlobalLog" xsi:type="File" fileName="${gdc:item=logDirectory}\Global.log" Layout="${longdate}|${level:uppercase=true}|${message}"/>
    <target name="GreatImportLog" xsi:type="File" fileName="${gdc:item=logDirectory}\GreatImport.log" Layout="${longdate}|${level:uppercase=true}|${message}"/>    
  </targets>
  <rules>
    <logger name="*" minlevel="Info" writeTo="ConsoleLog" />
    <logger name="GreatImport" minlevel="Debug" writeTo="GreatImportLog" final="true"/>
    <logger name="*" minlevel="Debug" writeTo="GlobalLog" />
  </rules>
</nlog>